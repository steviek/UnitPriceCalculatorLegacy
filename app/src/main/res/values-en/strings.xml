  <string name="all_changes_saved">All changes are saved. From now on, the save button will indicate when you have changes to save.\n\nThis message will disappear and not return in %1$dâ€¦</string>
  <!-- Label for a button that creates a new note for a row in the comparison -->
  <string name="add_note">Add note</string>
  <!-- Label for a button the edits an existing note for a row in the comparison -->
  <string name="edit_note">Edit note</string>
  <!-- Hint text for the input field where the user enters the note -->
  <string name="enter_note_hint">Enter a note</string>
  <!-- Header for a setting item controlling dark mode in the app -->
  <string name="dark_mode">Dark mode</string>
  <!-- Option that means the app matches the device-wide setting for dark mode -->
  <string name="dark_mode_match_device">Match device</string>
  <!-- Option that means the app will always be in dark mode -->
  <string name="dark_mode_always_dark">Always dark</string>
  <!-- Option that means the app will always be in light mode -->
  <string name="dark_mode_always_light">Always light</string>
  <!-- Header for a section that lets the user change the default comparison quantity for a unit
       type. For example, the default for weight is preset to 100 g, but the user can change this.
       -->
  <string name="default_unit_quantity">Default unit quantity</string>
  <!-- e.g. Weight: 100 g -->
  <string name="default_unit_quantity_summary_line">%1$s: %2$s %3$s</string>
  <!-- Title for a dialog that lets the user change the default comparison quantity for a unit type
       -->
  <string name="default_quantity_dialog_header">Default quantity: %1$s</string>
  <!-- Label for a button that lets the user search their saved comparisons -->
  <string name="search">Search</string>

  <!-- Header for a settings item that lets the user change which screen the app first load to. -->
  <string name="initial_screen">Initial screen</string>

  <!-- Title for a saved comparison. Name and number of items. -->
  <plurals name="saved_comparison_items">
    <item quantity="one">%1$s (%2$d item)</item>
    <item quantity="other">%1$s (%2$d items)</item>
  </plurals>

  <!-- Subtitle for a saved comparison, describing the best item. -->
  <string name="saved_comparison_subtitle">Best item: %1$s</string>
