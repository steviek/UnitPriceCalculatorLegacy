  <string name="untitled_italics"><i>Untitled</i></string>
  <!-- Prompt that lets the user know about the save feature -->
  <string name="unsaved_changes">You have unsaved changes. Click on the save button to save.</string>
  <!-- Onboarding text that lets the user know how the save feature works after they try it for the
